    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "site": "ng serve site --open",
    "dev-app": "ng serve dev-app --open",
    "dev": "npm run dev-app",
    "demo": "ng serve demo --open",
    "docs:build": " ts-node -P ./scripts/build/tsconfig.json ./scripts/build/site.build.ts",
    "site:build": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build site --prod && npm run docs:build",
    "demo:build": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build demo --prod",
    "common:build": "ng build common --prod && ts-node -P ./scripts/build/tsconfig.json ./scripts/build/common.build",
    "components:build": "ng build components --prod && ts-node -P ./scripts/build/tsconfig.json ./scripts/build/components.build",
    "drag-drop:build": "ng build drag-drop --prod",
    "g2-charts:build": "ng build g2-charts --prod && npx ts-node -P ./scripts/build/tsconfig.json ./scripts/build/g2-charts.build.ts",
    "libs:build": "npm run common:build && npm run components:build &&  npm run drag-drop:build &&  npm run g2-charts:build",
    "libs:publish": "./scripts/publish/libs.publish.sh",
    "mock": "ng serve mock-api",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
